<h1>View Users</h1>
<hr />
<div class="row">
  <div class="col s12 m6">
    <ul>
      <% @users.each do |user| %>
        <li
          data-id=<%= user.id %>
          data-name="<%= user.first_name %>"
          data-name="<%= user.last_name %>"
          data-phone="<%= user.phone_number %>"
        >
          <%= user.first_name %> 
        </li>
      <% end %>
    </ul>
  </div>
